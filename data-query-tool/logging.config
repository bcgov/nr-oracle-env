[loggers]
keys=root,data_query_tool_oralib,data_query_migration_files,types,batch_run_deps,oralib2
[handlers]
keys=consoleHandler

[formatters]
keys=consoleFormatter

#----- LOGGERS

[logger_root]
level=DEBUG
handlers=consoleHandler

[logger_data_query_tool_oralib]
level=DEBUG
handlers=consoleHandler
qualname=data_query_tool.oralib
propagate=0

[logger_data_query_migration_files]
level=DEBUG
handlers=consoleHandler
qualname=data_query_tool.migration_files
propagate=0

[logger_types]
level=DEBUG
handlers=consoleHandler
qualname=data_query_tool.types
propagate=0

[logger_batch_run_deps]
level=DEBUG
handlers=consoleHandler
qualname=batch_run_deps
propagate=0

[logger_oralib2]
level=DEBUG
handlers=consoleHandler
qualname=data_query_tool.oralib2
propagate=0



#----- HANDLERS

[handler_consoleHandler]
class=StreamHandler
formatter=consoleFormatter
args=(sys.stdout,)

#----- FORMATTERS

[formatter_consoleFormatter]
format=%(asctime)s - %(lineno)s - %(name)s - %(levelname)s - %(message)s
datefmt=%Y-%m-%d %H:%M:%S
class=logging.Formatter