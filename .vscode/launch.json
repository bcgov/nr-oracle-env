        {
            "name": "Python: Debug Pytest",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/data-population/tests",
            "console": "integratedTerminal",
            "env": {
                "MY_ENV_VAR": "value1",
                "DEBUG_MODE": "true"
            }
        },


        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "${command:pickArgs}"
            ]
        },
